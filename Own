class Solution {
    int isPalindrome(String S) {
        String x=S;
        String p="";
        x=x+" ";
        for(int i=0;i<x.length();i++)
        {
            char c=x.charAt(i);
            if(c!=' ')
            p=c+p;
            
        }
        if(p.equals(S))
        return 1;
        else 
        return 0;// code here
    }
};
